{
    "name": "lakremon/total-faker",
    "type": "library",
    "description": "total-faker is PHP program complex to take realistic data about peoples, Companies etc. to test your application.",
    "keywords": [
        "php",
        "faker",
        "test",
        "random"
    ],
    "homepage": "https://github.com/Lakremon/total-faker",
    "authors": [
        {
            "name": "Lakremon",
            "email": "self@lakremon.ru",
            "homepage": "https://lakremon.ru",
            "role": "Developer"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": "^7.2.0"
    },
    "autoload": {
        "psr-4": {
            "OpenApi\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "OpenApiTests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit && phpcs -p --extensions=php --standard=PSR2 --error-severity=1 --warning-severity=0 ./src ./tests"
    }
}