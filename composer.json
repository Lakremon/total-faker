{
    "name": "lakremon/total-faker",
    "type": "library",
    "license": "GPL-3.0-only",
    "description": "total-faker is PHP program complex to take realistic data about peoples, Companies etc. to test your application.",
    "keywords": [
        "php",
        "faker",
        "test",
        "random"
    ],
    "homepage": "https://github.com/Lakremon/total-faker",
    "authors": [
        {
            "name": "Lakremon",
            "email": "self@lakremon.ru",
            "homepage": "https://lakremon.ru",
            "role": "Developer"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": "^7.2.0"
    },
    "autoload": {
        "psr-4": {
            "TotalFaker\\": "src/",
            "TotalFaker\\Exceptions\\": "Exceptions/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload-dev": {
        "psr-4": {
            "TotalFakerTests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php ./vendor/phpunit/phpunit/phpunit"
    }
}